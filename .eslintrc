{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint-config-airbnb",
        "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "react/jsx-filename-extension": "off",
        "react/prop-types": "off"
    },
    "root": true
}
